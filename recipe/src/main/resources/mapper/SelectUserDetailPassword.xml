<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.entity.result">
	<select id="SelectUserDetailPassword" resultType="com.example.entity.SelectUserDetailPasswordEntity">
		SELECT
		    password
		FROM
		    public.user_detail 
		WHERE
		    user_id = #{userId}
		    and email_adress = #{emailAdress}
	</select>
</mapper>