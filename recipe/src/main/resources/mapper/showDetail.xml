<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.showDetail">
	<resultMap id='detailResultMap' type='SelectRecipeDetailEntity'>
	
	</resultMap>
	<select id='' resultType='detailResultMap'>
		select
		    recipe_name
		    , recipe_img
		    , recipe_descrip
		    , recipe_mainTxt
		    , register_date
		    , update_date 
		from
		    recipe_info 
		where
		    recipe_id = #{recipeId}
	</select>
</mapper>